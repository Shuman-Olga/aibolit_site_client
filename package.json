  "scripts": {
    "start": "webpack serve --mode development",
    "build": "webpack --mode production && react-snap"
  },
  "reactSnap": {
    "skipThirdPartyRequests": true,
    "puppeteer": {
      "timeout": 90000
    },
    "puppeteerArgs": [
      "--no-sandbox",
      "--disable-setuid-sandbox"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">1%",
      "not dead",
      "not op_mini all"
